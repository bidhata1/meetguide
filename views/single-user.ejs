<section class="blog_wrapper">

<div class="container user-header-wrapper">
    <div class="user-header-inner">
      <div class="uh-left">
        <div class="uh-image", style="padding-top: 20px;">
          <img class="uh-image-inner" src="https://i.imgur.com/Qv1WDJq.jpg" alt="">
          <div class="gradient"></div>
        </div>
      </div>
      <div class="uh-right">
        <div class="user-info">
            <h2 style="color:#f04c25; font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif">
              <%=user.name%>
              <svg class="uname-verified" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1350.03 1326.16">
                                  <defs><style>.cls-11{fill:var(--green);}.cls-12{fill:#ffffff;}</style></defs><title>verified</title>
                                  <g id="Layer_3" data-name="Layer 3">
                                      <polygon class="cls-11" points="0 747.37 120.83 569.85 70.11 355.04 283.43 292.38 307.3 107.41 554.93 107.41 693.66 0 862.23 120.83 1072.57 126.8 1112.84 319.23 1293.35 399.79 1256.05 614.6 1350.03 793.61 1197.87 941.29 1202.35 1147.15 969.64 1178.48 868.2 1326.16 675.02 1235.17 493.77 1315.72 354.99 1133.73 165.58 1123.29 152.16 878.64 0 747.37"/></g>
                                  <g id="Layer_2" data-name="Layer 2">
                                      <path class="cls-12" d="M755.33,979.23s125.85,78.43,165.06,114c34.93-36,234.37-277.22,308.24-331.94,54.71,21.89,85,73.4,93,80.25-3.64,21.89-321.91,418.58-368.42,445.94-32.74-3.84-259-195.16-275.4-217C689.67,1049.45,725.24,1003.85,755.33,979.23Z" transform="translate(-322.83 -335.95)"/></g>
                              </svg>
                              
                            </h2>
                        </div>
                        <h5 style="font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif">
                            <i class="fa fa-map-marker list-icon"></i>  <%=user.address%></h5> 
        
      </div>
    </div>
    </div>
    <div class="user-stories">
    <div class="user-stories-inner">
        <%user.interests && user.interests.forEach(int=>{%>
      <div class="story-wrapper">
        <div class="story-inner">
          <img class="story-img" src="https://images.unsplash.com/photo-1588110679566-158c6dea107c?ixlib=rb-1.2.1&auto=format&fit=crop&w=1990&q=80" alt="">
          <div class="gradient"></div>
        </div>
        <h2><%=int%> âœŒ</h2>
      </div>
  <%})%>
    </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-md-7 " style=" font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif; text-align: left; padding: 20px">
            <h4 class="user-info-heading">About me:</h4>
                <h5 style="font-weight: normal;">
                    <%=user.description%></h5>    
            <span style="padding: 10px;"></span> 
            <h4 class="user-info-heading">Contact:</h4>
            <h5 style="font-weight: normal;">
                <%=user.contact%></h5> 
                <span style="padding: 10px;"></span> 
                <h4 class="user-info-heading">Languages known:</h4>
                <h5 style="font-weight: normal;">
                    <%user.languages.forEach(lan=>{%>
                        <span class="badge badge-certified"><%=lan%></span>
                        <%})%>
                    </h5> 
                    <span style="padding: 10px;"></span> 

            </div>
            <div class="col-md-5 post_block" style=" font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif; text-align: left; padding: 20px">
                <p>
                    <span class="fa fa-star checked"></span>
                    <span class="fa fa-star checked"></span>
                    <span class="fa fa-star checked"></span>
                    <span class="fa fa-star"></span>
                    <span class="fa fa-star"></span>
                    <h6 style="text-align: left; margin-top: -10px;"><%= user.reviews && user.reviews.length ? user.reviews.length : 0 %> reviews</h6>
               </p>
               <span style="padding: 10px;"></span> 

                <h4 class="user-info-heading">Rate: <span style="color: #414141;">$<%=user.rate%>/hr </span></h4> 
                <div class="row">
                    
                    <div class="col-lg-6">
                       
                      <div class="input-group">
                        <span class="input-group-addon" id="sizing-addon2">From</span>

                        <input type="date" class="form-control" placeholder="From">
                      </div><!-- /input-group -->
                    </div><!-- /.col-lg-6 -->
                    <div class="col-lg-6">
                      <div class="input-group">
                        <span class="input-group-addon" id="sizing-addon2">To</span>

                        <input type="date" class="form-control" placeholder="To">
                        
                      </div><!-- /input-group -->
                    </div><!-- /.col-lg-6 -->
                  </div><!-- /.row -->
                  <span style="padding: 10px;"></span> 

                  <select class="form-control form-control-lg">
                    <option>Sunday</option>
                    <option>Monday</option>
                    <option>Tuesday</option>
                    <option>Wednesday</option>
                    <option>Thursday</option>
                    <option>Friday</option>
                    <option>Saturday</option>
                  </select>
                    <button type="button" class="btn btn-danger bookme">Book me</button>
            </div>
        </div>

    
    </div>
   
    <div class="space"></div>
    <section class="blog_wrapper container">
        
        <div class="row">
            <h4 class="user-info-heading" style="text-align: center;">What people say about me:</h4>
            <div class="col-md-12">
                <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="0">
                <!-- Carousel indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                </ol>   
                <!-- Wrapper for carousel items -->
                <div class="carousel-inner">
                    <div class="item carousel-item active">
                        <div class="row">
                            <%user.reviews.forEach(r=>{%>
                            <div class="col-sm-4">
                                <div class="thumb-wrapper">
                                    <div class="img-box">
                                        <img src="<%=r.src%>" alt="">
                                    </div>
                                    <div class="thumb-content">
                                        <h4><%=r.email%></h4>
                                        <h5><%=r.review%></h5>>
                                        <div class="star-rating">
                                            <ul class="list-inline">
                                                <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                <li class="list-inline-item"><i class="fa fa-star-o"></i></li>
                                            </ul>
                                        </div>
                                    </div>						
                                </div>
                            </div>
                          <%})%>
                        </div>
                    </div>
                    <div class="item carousel-item">
                        <div class="row">
                           					
                        </div>
                    </div>
                    <div class="item carousel-item">
                        <div class="row">
                           					
                        </div>
                    </div>
                </div>
                <!-- Carousel controls -->
                <a class="carousel-control left carousel-control-prev" href="#myCarousel" data-slide="prev">
                    <i class="fa fa-angle-left"></i>
                </a>
                <a class="carousel-control right carousel-control-next" href="#myCarousel" data-slide="next">
                    <i class="fa fa-angle-right"></i>
                </a>
            </div>
            </div>
        </div>
    <section>

</section>
