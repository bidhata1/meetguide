                            <ul>
                                <li><a href="">Travel</a></li>
                                <li><a href="">Travel</a></li>
                                <li><a href="">Travel</a></li>
                                <li><a href="">Travel</a></li>
                            </ul>

                        </div>


                        <div class="widget">
                            <h2 class="heading">Recent Post</h2>

                            <ul>
                                <li><a href="">Travel</a></li>
                                <li><a href="">Travel</a></li>
                                <li><a href="">Travel</a></li>
                                <li><a href="">Travel</a></li>
                            </ul>

                        </div>


                    </div>
                </div>
                <!--Sidebar End Here-->

            </div>
        </div>
